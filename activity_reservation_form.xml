<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RoomDeliveryForm"
    android:background="#ffffff">


    <ImageButton
        android:id="@+id/cancel"
        android:layout_width="72dp"
        android:layout_height="59dp"
        android:layout_marginStart="708dp"
        android:layout_marginLeft="708dp"
        android:layout_marginTop="24dp"
        android:background="@color/cardview_dark_background"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/close_button_2" />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="12dp"
        tools:layout_editor_absoluteY="40dp">

        <View
            android:layout_width="370dp"
            android:layout_height="2dp"
            android:layout_marginLeft="75dp"
            android:layout_marginTop="300dp"
            android:background="@color/cardview_dark_background"/>

        <View
            android:layout_width="200dp"
            android:layout_height="2dp"
            android:layout_marginLeft="75dp"
            android:layout_marginTop="115dp"
            android:background="@color/cardview_dark_background" />

        <View
            android:layout_width="400dp"
            android:layout_height="2dp"
            android:layout_marginLeft="75dp"
            android:layout_marginTop="120dp"
            android:background="@color/cardview_dark_background"/>

        <View
            android:layout_width="400dp"
            android:layout_height="2dp"
            android:layout_marginLeft="75dp"
            android:layout_marginTop="120dp"
            android:background="@color/cardview_dark_background"/>

        /><![CDATA[

        />


    ]]>
    </LinearLayout>



    <TextView
        android:id="@+id/title1"
        android:layout_width="628dp"
        android:layout_height="77dp"
        android:layout_marginTop="96dp"
        android:fontFamily="@font/adamina"
        android:gravity="center"
        android:text="Reservation for"
        android:textColor="#000000"
        android:textSize="48sp"
        android:textStyle="bold"
        android:typeface="serif"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/table_number"
        android:layout_width="404dp"
        android:layout_height="77dp"
        android:layout_marginTop="96dp"
        android:fontFamily="@font/adamina"
        android:gravity="center"
        android:text="Table #"
        android:textColor="#000000"
        android:textSize="48sp"
        android:textStyle="bold"
        android:typeface="serif"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.988"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="240dp"
        android:background="#ffffff"
        android:ems="10"
        android:hint="Name"
        android:inputType="textCapSentences|text"
        android:textSize="32sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.184"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/party_size"
        android:layout_width="200dp"
        android:layout_height="48dp"
        android:layout_marginStart="84dp"
        android:layout_marginLeft="84dp"
        android:layout_marginTop="352dp"
        android:background="#ffffff"
        android:ems="10"
        android:hint="Party Size"
        android:inputType="number"
        android:textSize="32sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="472dp"
        android:background="#ffffff"
        android:ems="10"
        android:hint="Cellphone #"
        android:inputType="phone"
        android:textSize="32sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.184"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/time"
        android:layout_width="200dp"
        android:layout_height="48dp"
        android:layout_marginStart="84dp"
        android:layout_marginLeft="84dp"
        android:layout_marginTop="588dp"
        android:background="#ffffff"
        android:ems="10"
        android:hint="Time"
        android:inputType="textCapSentences|text"
        android:textSize="32sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/special_instructions_border"
        android:layout_width="693dp"
        android:layout_height="208dp"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="740dp"
        android:background="@color/cardview_dark_background"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.362"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/special_requests"
        android:layout_width="688dp"
        android:layout_height="202dp"
        android:layout_marginStart="60dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="740dp"
        android:background="#ffffff"
        android:ems="10"
        android:gravity="center_horizontal"
        android:hint="Special Requests (Optional)"
        android:inputType="textCapSentences|text"
        android:padding="3dp"
        android:textAlignment="center"
        android:textSize="32sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/submit"
        android:layout_width="300dp"
        android:layout_height="75dp"
        android:layout_marginStart="252dp"
        android:layout_marginLeft="252dp"
        android:layout_marginTop="1076dp"
        android:background="#FF5F15"
        android:text="Submit!"
        android:textColor="@color/white"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>


